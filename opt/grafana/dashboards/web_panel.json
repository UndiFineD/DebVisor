  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(flask_http_request_total[1m])",
          "legendFormat": "{{method}} {{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Request Latency (p95)",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(flask_http_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate (5xx)",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "rate(flask_http_request_total{status=~\"5..\"}[1m])",
          "legendFormat": "{{method}} {{path}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Active Users",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "flask_active_sessions",
          "legendFormat": "Users",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 16,
  "style": "dark",
